{
    "name": "GitHub Pages Dev environment",
    "build": {
        "dockerfile": "Dockerfile.dev",
        "context": "..",
        "args": {}
    },
    "forwardPorts": [4000],
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached",
    "workspaceFolder": "/workspace",
    "customizations": {
      "vscode": {
        "extensions": ["rebornix.Ruby"]
      }
    }
  }